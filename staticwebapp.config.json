{
  "routes": [
    {
      "route": "/api/*",
      "allowedRoles": ["anonymous"],
      "headers": {
        "Cache-Control": "no-cache"
      }
    },
    {
      "route": "/*",
      "allowedRoles": ["anonymous"],
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html"
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html",
      "statusCode": 200
    }
  },
   "platform": {
    "apiRuntime": "node:18"
  }
}
